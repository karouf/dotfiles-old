alias gl="git log"
alias gs="git status"
alias gc="git commit"
alias ga="git add"
alias gco="git checkout"
alias gb="git branch"
alias gdc="git diff --color"
alias gdcm="git diff --cached --color"
alias gg="git log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s" --branches"
